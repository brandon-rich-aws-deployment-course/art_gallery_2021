             "Action": [
                 "ssm:DescribeParameters"
             ],
             "Resource": "*"
         },
         {
             "Effect": "Allow",
             "Action": [
                 "ssm:GetParameters"
             ],
             "Resource": [
                 "arn:aws:ssm:us-east-1:678020273738:parameter/CodeDeploy*"
             ]
         },
         {
             "Effect": "Allow",
             "Action": [
                 "kms:Decrypt"
             ],
             "Resource": "arn:aws:kms:us-east-1:678020273738:alias/aws/ssm"
         }
     ]
 }
